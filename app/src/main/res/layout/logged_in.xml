<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="loggedIn"
            type="fullerton.lfg.ui.loggedin.LoggedIn" />
        <variable
            name="loggedInviewModel"
            type="fullerton.lfg.ui.loggedin.LoggedInViewModel" />
        <variable
            name="loggedInUserview"
            type="fullerton.lfg.ui.loggedin.LoggedInUserView" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.loggedin.LoggedIn">

        <TextView
            android:id="@+id/greeting"
            android:layout_width="271dp"
            android:layout_height="60dp"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.215" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>